<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="sryx.dao.UserMapper">
  <insert id="saveUser" parameterType="sryx.pojo.User" >
    insert into 
    user (name, pwd, age, creatTime)
    values ( #{name}, #{pwd}, #{age},#{creatTime})
  </insert>
</mapper>
